package domain;

public class MarineBean extends GoundUnitBean{
	private String gun;

	public String getGun() {
		return gun;
	}

	public void setGun(String gun) {
		this.gun = gun;
	}

	@Override
	public String toString() {
		return "마린 [총=" + gun + ", 이름=" + name + ", 타격점=" + hitpoint + ", 체력=" + hp + "]";
	}
}
