package controller;

public class StarcraftController {
	public static void main(String[] args) {
		
	
	while(true) {
		String select = "0.종료 , 1.Dropship ,2.Marine, 3.SCV, 4.Tank " ;
		switch(select) {
			case "0" : return;
			case "1" : break;
			case "2" : break;
			case "3" : break;
			case "4" : break;
			case "5" : break;
			default : break;
				
		}
	}
}
}
